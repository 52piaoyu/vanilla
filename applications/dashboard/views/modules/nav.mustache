<ul class="nav {{navCssClass}}">
  {{#items}}
      {{#isGroup}}
          <li role="presentation" class="header {{headerCssClass}}">
            {{#headerIcon}}
                <span class="icon {{headerIcon}}"></span>
            {{/headerIcon}}
            {{headerText}}
            {{#headerBadge}}
                <span class="badge">{{headerBadge}}</span>
            {{/headerBadge}}
          </li>
      {{/isGroup}}
      {{#isLink}}
          <li role="presentation" {{#listItemCssClass}}class="{{listItemCssClass}}"{{/listItemCssClass}}>
              <a class="{{linkCssClass}}" href="{{linkUrl}}">
                {{#linkIcon}}
                    <span class="icon {{linkIcon}}"></span>
                {{/linkIcon}}
                {{linkText}}
                {{#linkBadge}}
                    <span class="badge">{{linkBadge}}</span>
                {{/linkBadge}}
              </a>
          </li>
      {{/isLink}}
      {{#isDivider}}
          <li role="presentation" class="divider {{dividerCssClass}}"></li>
      {{/isDivider}}
      {{#isDropdown}}
        {{#dropdownmenu}}
          {{>dropdown}}
        {{/dropdownmenu}}
      {{/isDropdown}}
      {{#hasChildren}}
          <li role="presentation" {{#listItemCssClass}}class="{{listItemCssClass}}"{{/listItemCssClass}}>
            {{>nav}}
          </li>
      {{/hasChildren}}
  {{/items}}
</ul>

